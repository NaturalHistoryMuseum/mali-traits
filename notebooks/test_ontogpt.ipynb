    "def ground_to_flopo(label, ontology):\n",
    "    for term in ontology.terms():\n",
    "        if term.name and label.lower() in term.name.lower():\n",
    "            return term.id\n",
    "        for synonym in term.synonyms:\n",
    "            if label.lower() in synonym.description.lower():\n",
    "                return term.id\n",
    "    return \"NOT FOUND\""
   ]
