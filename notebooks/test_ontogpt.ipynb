    "def extract_traits_openai(description, FLOPO_PROMPT):\n",
    "    client = OpenAI(api_key=openai_api_key)\n",
    "\n",
    "    prompt = FLOPO_PROMPT.format(description=description)\n",
    "    response = client.chat.completions.create(\n",
    "        model=\"gpt-4o\",\n",
    "        messages=[\n",
    "            {\"role\": \"system\", \"content\": \"You are ChatGPT, a helpful assistant that extracts and maps plant traits to the FLOPO ontology.\"},\n",
    "            {\"role\": \"user\", \"content\": prompt}\n",
    "        ],\n",
    "        temperature=0.2,\n",
    "    )\n",
    "    return response.choices[0].message.content"
   ]
