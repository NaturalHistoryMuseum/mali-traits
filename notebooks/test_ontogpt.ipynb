   "source": [
    "import anthropic\n",
    "\n",
    "def extract_traits_claude(text):\n",
    "    # prompt = f\"Extract plant phenotypic descriptors (e.g., shape, color, texture) from this sentence:\\n{text}\\nList each trait as a line.\"\n",
    "\n",
    "    prompt = f\"\"\"Extract *individual* plant phenotypic traits from the following sentence. \n",
    "    Each trait should be atomic and expressed separately, one per line. \n",
    "    Avoid combining traits.\n",
    "\n",
    "    Example:\n",
    "    Input: \"The plant is tufted and shortly rhizomatous or stoloniferous.\"\n",
    "    Output:\n",
    "    - plant is tufted\n",
    "    - plant is shortly rhizomatous\n",
    "    - plant is stoloniferous\n",
    "\n",
    "    Text:\n",
    "    {text}\n",
    "    List each trait below:\n",
    "    \"\"\"\n",
    "\n",
    "    client = anthropic.Anthropic(\n",
    "        # defaults to os.environ.get(\"ANTHROPIC_API_KEY\")\n",
    "        api_key=\"sk-ant-api03---hjxdrQAA\",\n",
    "    )\n",
    "    \n",
    "    response = client.messages.create(\n",
    "        model=\"claude-sonnet-4-20250514\",\n",
    "        max_tokens=20000,\n",
    "        temperature=0.3,\n",
    "        messages=[{\"role\": \"user\", \"content\": prompt}]\n",
    "    )\n",
    "    \n",
    "    # Claude's messages.create returns an object with 'content' as a list of TextBlocks\n",
    "    message_text = \"\".join(block.text for block in response.content)\n",
    "\n",
    "    return [trait.strip(\"- \").strip() for trait in message_text.strip().split(\"\\n\") if trait.strip()]"
   ]
  },
