    "def extract_traits_openai_rag(query, retrieval_context):\n",
    "    prompt = f\"\"\"You are a plant ontology assistant. You will be given a plant description, and you should extract *individual* plant phenotypic traits from it.\n",
    "    Each trait should be atomic, expressed separately, and should match or closely align with one or more of the FLOPO entries listed below. Avoid combining multiple traits.\n",
    "    \n",
    "    FLOPO Reference Context:\n",
    "    {retrieval_context}\n",
    "    \n",
    "    Example:\n",
    "    Input: \"The plant is tufted and shortly rhizomatous or stoloniferous.\"\n",
    "    Output:\n",
    "    - plant is tufted\n",
    "    - plant is shortly rhizomatous\n",
    "    - plant is stoloniferous\n",
    "    \n",
    "    Now extract traits from the following text:\n",
    "    \"{query}\"\n",
    "    \n",
    "    List each trait below:\n",
    "    \"\"\"\n",
    "    client = OpenAI(api_key=openai_api_key)\n",
    "    \n",
    "    response = client.chat.completions.create(\n",
    "        model=\"gpt-4\",\n",
    "        messages=[{\"role\": \"user\", \"content\": prompt}],\n",
    "        temperature=0.3\n",
    "    )\n",
    "    content = response.choices[0].message.content.strip()\n",
    "    traits = [line.strip(\"- \").strip() for line in content.split('\\n') if line.strip()]\n",
    "    return traits"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 91,
   "metadata": {},
   "outputs": [],
   "source": [
    "def build_flopo_index(flopo_df, model_name=\"all-MiniLM-L6-v2\"):\n",
    "    corpus_texts = [f\"{row['label']}: {row['desc']}\" for _, row in flopo_df.iterrows()]\n",
    "    # Embed corpus\n",
    "    model = SentenceTransformer(model_name)\n",
    "    embeddings = model.encode(corpus_texts, convert_to_numpy=True)\n",
    "    # Build FAISS index\n",
    "    index = faiss.IndexFlatL2(embeddings.shape[1])\n",
    "    index.add(embeddings)\n",
    "\n",
    "    return model, index, corpus_texts\n",
    "\n",
    "def retrieve_context(query, model, index, corpus_texts, top_k=3):\n",
    "    query_embedding = model.encode([query], convert_to_numpy=True)\n",
    "    D, I = index.search(query_embedding, top_k)\n",
    "    top_entries = [corpus_texts[i] for i in I[0]]\n",
    "    return \"\\n\".join(top_entries)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 61,
   "metadata": {},
   "outputs": [],
   "source": [
    "def retrieve_flopo_context(query, model, faiss_index, corpus_texts, flopo_df, top_k=10):\n",
    "    query_emb = model.encode([query], convert_to_numpy=True)\n",
    "    D, I = faiss_index.search(query_emb, top_k)\n",
    "    \n",
    "    context_entries = []\n",
    "    metadata_map = {}\n",
    "    for idx in I[0]:\n",
    "        label = flopo_df.iloc[idx][\"label\"]\n",
    "        desc = flopo_df.iloc[idx].get(\"desc\", \"\")\n",
    "        uri = flopo_df.iloc[idx][\"uri\"]\n",
    "        text = f\"{label}: {desc}\"\n",
    "        context_entries.append(text)\n",
    "        metadata_map[label.lower()] = uri  # lowercased for fuzzy match\n",
    "\n",
    "    retrieval_context = \"\\n\".join(context_entries)\n",
    "    return retrieval_context, metadata_map\n"
   ]
  },
