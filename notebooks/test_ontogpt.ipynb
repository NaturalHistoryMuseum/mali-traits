   "source": [
    "def extract_atomic_traits_openai(text):\n",
    "    prompt = f\"\"\"Extract *individual* plant phenotypic traits from the following sentence. \n",
    "    Each trait should be atomic and expressed separately, one per line. \n",
    "    Avoid combining traits.\n",
    "\n",
    "    Example:\n",
    "    Input: \"The plant is tufted and shortly rhizomatous or stoloniferous.\"\n",
    "    Output:\n",
    "    - plant is tufted\n",
    "    - plant is shortly rhizomatous\n",
    "    - plant is stoloniferous\n",
    "\n",
    "    Text:\n",
    "    {text}\n",
    "    List each trait below:\n",
    "    \"\"\"\n",
    "    client = OpenAI(api_key=openai_api_key)\n",
    "    response = client.chat.completions.create(\n",
    "        model=\"gpt-4\",\n",
    "        messages=[{\"role\": \"user\", \"content\": prompt}],\n",
    "        temperature=0.3\n",
    "    )\n",
    "    content = response.choices[0].message.content.strip()\n",
    "    traits = [line.strip(\"- \").strip() for line in content.split('\\n') if line.strip()]\n",
    "    return traits"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 107,
   "metadata": {},
   "outputs": [],
   "source": [
    "def match_traits_to_flopo_faiss(traits, faiss_index, flopo_df, model, top_k=1):\n",
    "    matched = []\n",
    "    corpus_labels = flopo_df['label'].tolist()\n",
    "    corpus_uris = flopo_df['uri'].tolist()\n",
    "    \n",
    "    for trait in traits:\n",
    "        emb = model.encode([trait], convert_to_numpy=True)\n",
    "        D, I = faiss_index.search(emb, top_k)\n",
    "        \n",
    "        for idx, score in zip(I[0], D[0]):\n",
    "            matched.append({\n",
    "                \"phenotypic_trait\": trait,\n",
    "                \"flopo_label\": corpus_labels[idx],\n",
    "                \"flopo_uri\": corpus_uris[idx],\n",
    "                \"flopo_id\": corpus_uris[idx].split(\"/\")[-1].replace(\"_\", \":\"),\n",
    "                \"flopo_faiss_score\": float(score)\n",
    "            })\n",
    "    return matched"
   ]
  },
