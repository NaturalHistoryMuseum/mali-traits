    "def majority_vote_semantic_verbose(tables, threshold=0.8, vote_threshold=2, save_csv_path=None):\n",
    "\n",
    "    model = SentenceTransformer(\"all-MiniLM-L6-v2\")\n",
    "\n",
    "    def compute_similarity(a, b):\n",
    "        embeddings = model.encode([a, b], convert_to_tensor=True)\n",
    "        return float(util.pytorch_cos_sim(embeddings[0], embeddings[1]))\n",
    "\n",
    "    # Preprocess unique plant parts from all tables\n",
    "    all_parts = {\n",
    "        row.get(\"plant_part\", \"\")\n",
    "        for table in tables\n",
    "        for row in table\n",
    "        if isinstance(row, dict)\n",
    "    }\n",
    "    plant_part_map = {p: normalise_plant_part_fuzzy(p) for p in all_parts}\n",
    "\n",
    "    merged = []\n",
    "    for table in tables:\n",
    "        for row in table:\n",
    "            row = {k.lower(): v for k, v in row.items()}\n",
    "            normalised_part = plant_part_map.get(row.get(\"plant_part\", \"\"), \"\")\n",
    "            merged.append({\n",
    "                \"trait\": row[\"trait\"].strip(),\n",
    "                \"value\": row[\"value\"].strip(),\n",
    "                \"term\": row[\"ontology_id\"].strip(),\n",
    "                \"phenotypic_trait\": row.get(\"phenotypic_trait\", \"\").strip(),\n",
    "                \"plant_part\": normalised_part\n",
    "            })\n",
    "            \n",
    "    # === Precompute embeddings ===\n",
    "    trait_texts = [entry[\"trait\"] for entry in merged]\n",
    "    value_texts = [entry[\"value\"] for entry in merged]\n",
    "    trait_embeddings = model.encode(trait_texts, convert_to_tensor=True)\n",
    "    value_embeddings = model.encode(value_texts, convert_to_tensor=True)\n",
    "\n",
    "    # === Clustering with precomputed embeddings ===\n",
    "    clusters = []\n",
    "    used = [False] * len(merged)\n",
    "\n",
    "    for i, entry_i in enumerate(merged):\n",
    "        if used[i]:\n",
    "            continue\n",
    "        cluster = [entry_i]\n",
    "        used[i] = True\n",
    "        for j in range(i + 1, len(merged)):\n",
    "            if used[j]:\n",
    "                continue\n",
    "            same_part = entry_i[\"plant_part\"] == merged[j][\"plant_part\"]\n",
    "            if not same_part:\n",
    "                continue\n",
    "            trait_sim = util.cos_sim(trait_embeddings[i], trait_embeddings[j]).item()\n",
    "            value_sim = util.cos_sim(value_embeddings[i], value_embeddings[j]).item()\n",
    "            if trait_sim > threshold and value_sim > threshold:\n",
    "                cluster.append(merged[j])\n",
    "                used[j] = True\n",
    "        clusters.append(cluster)\n",
    "\n",
    "    consensus_rows = []\n",
    "    non_consensus_rows = []\n",
    "\n",
    "    for cluster in clusters:\n",
    "        trait = Counter([e[\"trait\"] for e in cluster]).most_common(1)[0][0]\n",
    "        value = Counter([e[\"value\"] for e in cluster]).most_common(1)[0][0]\n",
    "        part = Counter([e[\"plant_part\"] for e in cluster]).most_common(1)[0][0]\n",
    "        term_counts = Counter([e[\"term\"] for e in cluster])\n",
    "        phenotypic_counts = Counter([e[\"phenotypic_trait\"] for e in cluster])\n",
    "        most_common_term, count = term_counts.most_common(1)[0]\n",
    "        most_common_phenotypic = phenotypic_counts.most_common(1)[0][0]\n",
    "\n",
    "        row = {\n",
    "            \"trait\": trait,\n",
    "            \"value\": value,\n",
    "            \"plant_part\": part,\n",
    "            \"llm_mapped_term\": most_common_term,\n",
    "            \"phenotypic_trait\": most_common_phenotypic,\n",
    "            \"consensus_count\": 0,\n",
    "            \"notes\": \"\"\n",
    "        }\n",
    "\n",
    "        if count >= vote_threshold:\n",
    "            row[\"notes\"] = f\"Consensus (appeared {count} times)\"\n",
    "            row[\"consensus_count\"] = count\n",
    "            consensus_rows.append(row)\n",
    "        else:\n",
    "            row[\"notes\"] = f\"No consensus (top term appeared {count} times); alternatives: {dict(term_counts)}\"\n",
    "            row[\"consensus_count\"] = count\n",
    "            non_consensus_rows.append(row)\n",
    "\n",
    "    result_df = pd.DataFrame(consensus_rows + non_consensus_rows)\n",
    "\n",
    "    if save_csv_path:\n",
    "        result_df.to_csv(save_csv_path, index=False)\n",
    "\n",
    "    return result_df, pd.DataFrame(non_consensus_rows)\n"
   ]
  },
