    "def extract_structured_traits(text):\n",
    "    prompt = f\"\"\"Extract structured plant traits from this sentence:\n",
    "Text: \"{text}\"\n",
    "Return as JSON list with `plant_part` and `descriptor`.\n",
    "\n",
    "Example:\n",
    "[\n",
    "  {{ \"plant_part\": \"leaf\", \"descriptor\": \"ovate\" }},\n",
    "  {{ \"plant_part\": \"flower\", \"descriptor\": \"yellow\" }}\n",
    "]\n",
    "\"\"\"\n",
    "    response = openai.ChatCompletion.create(\n",
    "        model=\"gpt-4\",\n",
    "        messages=[{\"role\": \"user\", \"content\": prompt}],\n",
    "        temperature=0.2,\n",
    "    )\n",
    "    return eval(response.choices[0].message.content.strip())\n",
    "\n",
    "def match_descriptors_to_flopo(traits, knn, model, labels, label_dict):\n",
    "    results = []\n",
    "    for trait in traits:\n",
    "        desc = trait[\"descriptor\"].lower()\n",
    "        embedding = model.encode([desc])\n",
    "        distance, index = knn.kneighbors(embedding)\n",
    "        matched_label = labels[index[0][0]]\n",
    "        results.append({\n",
    "            \"plant_part\": trait[\"plant_part\"],\n",
    "            \"descriptor\": desc,\n",
    "            \"matched_label\": matched_label,\n",
    "            \"flopo_uri\": label_dict[matched_label],\n",
    "            \"distance\": float(distance[0][0])\n",
    "        })\n",
    "    return results"
   ]
