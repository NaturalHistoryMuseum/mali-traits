    "def summarize_ontology_mappings_from_dir(input_dir, term_column=\"llm_mapped_term\"):\n",
    "    records = []\n",
    "\n",
    "    for file in os.listdir(input_dir):\n",
    "        if not file.endswith(\".csv\") or file == \"trait_stats.csv\":\n",
    "            continue  # Skip summary file\n",
    "\n",
    "        filepath = os.path.join(input_dir, file)\n",
    "        try:\n",
    "            df = pd.read_csv(filepath)\n",
    "        except Exception as e:\n",
    "            print(f\"Failed to read {file}: {e}\")\n",
    "            continue\n",
    "\n",
    "        if term_column not in df.columns:\n",
    "            print(f\"Missing column '{term_column}' in {file}\")\n",
    "            continue\n",
    "\n",
    "        taxon = os.path.splitext(file)[0]\n",
    "        total_rows = len(df)\n",
    "        if total_rows == 0:\n",
    "            continue\n",
    "\n",
    "        matched_flopo = df[term_column].astype(str).str.startswith(\"FLOPO:\").sum()\n",
    "        matched_pato = df[term_column].astype(str).str.startswith(\"PATO:\").sum()\n",
    "        matched_total = matched_flopo + matched_pato\n",
    "        unmatched = total_rows - matched_total\n",
    "\n",
    "        records.append({\n",
    "            \"taxon\": taxon,\n",
    "            \"total_rows\": total_rows,\n",
    "            \"matched_flopo\": matched_flopo,\n",
    "            \"matched_pato\": matched_pato,\n",
    "            \"matched_total\": matched_total,\n",
    "            \"unmatched\": unmatched,\n",
    "            \"matched_%\": round(matched_total / total_rows * 100, 2),\n",
    "            \"flopo_%\": round(matched_flopo / total_rows * 100, 2),\n",
    "            \"pato_%\": round(matched_pato / total_rows * 100, 2)\n",
    "        })\n",
    "\n",
    "    return pd.DataFrame(records)"
   ]
