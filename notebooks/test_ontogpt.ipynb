   "source": [
    "'''\n",
    "for row in df\n",
    "    output_filename = row[taxon].csv\n",
    "    for i to num_run\n",
    "        run_i = parse_llm_csv_output(extract_traits_openai(row[description], FLOPO_PROMPT))\n",
    "    mean_run_len = (len(run1)+len(run2)+...+len(run_num_run))/num_run\n",
    "    result_df, conflicts_df = majority_vote_semantic_verbose(\n",
    "        tables=[run1, run2, ... run_num_run],\n",
    "        threshold=0.8,\n",
    "        vote_threshold=math.ceil(num_run/2),\n",
    "    )\n",
    "    matches = match_traits_to_flopo_faiss(result_df['phenotypic_trait'], index, flopo_labels, model)\n",
    "    flopo_matches = pd.DataFrame(matches)\n",
    "    matches = match_traits_to_pato_faiss(result_df['phenotypic_trait'], pato_index, pato_labels, pato_model)\n",
    "    pato_matches = pd.DataFrame(matches)\n",
    "    merge flopo_matches and pato_matches back to result_df\n",
    "    save_csv_path= \"output/output_filename\"\n",
    "'''\n",
    "import os\n",
    "import math\n",
    "from tqdm import tqdm\n",
    "\n",
    "def batch_extract_and_match_traits(df, num_runs, flopo_index, flopo_labels, flopo_model,\n",
    "                                   pato_index, pato_labels, pato_model, prompt_template,\n",
    "                                   similarity_threshold, output_dir=\"output\"):\n",
    "    os.makedirs(output_dir, exist_ok=True)\n",
    "    stats_records = []\n",
    "    stats_path = os.path.join(output_dir, \"trait_stats.csv\")\n",
    "\n",
    "    for _, row in tqdm(df.iterrows(), total=len(df), desc=\"Processing taxa\"):\n",
    "        taxon = row[\"taxon\"]\n",
    "        description = row[\"description\"]\n",
    "        output_filename = f\"{taxon}.csv\"\n",
    "        output_path = os.path.join(output_dir, output_filename)\n",
    "        \n",
    "        # Skip if already processed\n",
    "        if os.path.exists(output_path):\n",
    "            print(f\"Skipped (already exists): {taxon}\")\n",
    "            continue\n",
    "\n",
    "        # Run LLM multiple times\n",
    "        runs = []\n",
    "        for _ in range(num_runs):\n",
    "            response = extract_traits_openai(description, prompt_template)\n",
    "            parsed = parse_llm_csv_output(response)\n",
    "            runs.append(parsed)\n",
    "\n",
    "        run_lengths = [len(r) for r in runs]\n",
    "        mean_run_len = sum(run_lengths) / num_runs\n",
    "        # print(f\"Average traits extracted: {mean_run_len:.2f}\")\n",
    "\n",
    "        vote_threshold = math.ceil(num_runs / 2)\n",
    "        \n",
    "        result_df, conflicts_df = majority_vote_semantic_verbose(\n",
    "            tables=runs,\n",
    "            threshold=similarity_threshold,\n",
    "            vote_threshold=vote_threshold\n",
    "        )\n",
    "        # Deduplicate exact trait entries\n",
    "        result_df = result_df.drop_duplicates(subset=[\"trait\", \"value\", \"plant_part\", \"phenotypic_trait\"])\n",
    "\n",
    "        # Stats\n",
    "        full_match = len(result_df.query(f\"consensus_count == {num_runs}\"))\n",
    "        consensus = len(result_df.query(f\"consensus_count == {vote_threshold}\"))\n",
    "        conflict = len(result_df.query(f\"consensus_count < {vote_threshold}\"))\n",
    "        total_out = len(result_df)\n",
    "\n",
    "        stats_row = {\n",
    "            \"taxon\": taxon,\n",
    "            \"mean_run_len\": round(mean_run_len, 2),\n",
    "            \"total_out\": total_out,\n",
    "            \"full_match_%\": round(full_match / mean_run_len * 100, 2) if total_out else 0,\n",
    "            \"consensus_%\": round(consensus / total_out * 100, 2) if total_out else 0,\n",
    "            \"conflict_%\": round(conflict / total_out * 100, 2) if total_out else 0\n",
    "        }\n",
    "        stats_records.append(stats_row)\n",
    "\n",
    "        # Append to CSV (streaming stats)\n",
    "        pd.DataFrame([stats_row]).to_csv(\n",
    "            stats_path, mode=\"a\", header=not os.path.exists(stats_path), index=False\n",
    "        )\n",
    "        \n",
    "        # Match to FLOPO\n",
    "        flopo_matches = pd.DataFrame(\n",
    "            match_traits_to_flopo_faiss(result_df[\"phenotypic_trait\"], flopo_index, flopo_labels, flopo_model)\n",
    "        )\n",
    "\n",
    "        # Match to PATO\n",
    "        pato_matches = pd.DataFrame(\n",
    "            match_traits_to_pato_faiss(result_df[\"phenotypic_trait\"], pato_index, pato_labels, pato_model)\n",
    "        )\n",
    "\n",
    "        # Merge matches\n",
    "        merged = result_df.copy()\n",
    "        merged = merged.merge(flopo_matches, on=\"phenotypic_trait\", how=\"left\")\n",
    "        merged = merged.merge(pato_matches, on=\"phenotypic_trait\", how=\"left\")\n",
    "\n",
    "        # Save result\n",
    "        merged.to_csv(output_path, index=False)\n",
    "        # print(f\"Saved to: {output_path}\")\n",
    "    \n",
    "    # Final stats DataFrame\n",
    "    stats_df = pd.DataFrame(stats_records)\n",
    "    \n",
    "    return stats_df"
   ]
  },
