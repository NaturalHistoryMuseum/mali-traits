  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "def match_traits_to_flopo_hybrid(traits, flopo_labels, flopo_vectors, flopo_texts, nlp, spaCy_threshold=0.7, difflib_cutoff=0.6):\n",
    "    matched = []\n",
    "    for trait in traits:\n",
    "        cleaned = clean_trait(trait)\n",
    "        trait_doc = nlp(cleaned)\n",
    "\n",
    "        best_label = None\n",
    "        score = None\n",
    "        method = None\n",
    "\n",
    "        # Try spaCy vector similarity\n",
    "        if trait_doc.has_vector:\n",
    "            sims = np.dot(flopo_vectors, trait_doc.vector) / (norm(flopo_vectors, axis=1) * norm(trait_doc.vector) + 1e-8)\n",
    "            best_idx = np.argmax(sims)\n",
    "            best_score = sims[best_idx]\n",
    "\n",
    "            if best_score >= spaCy_threshold:\n",
    "                best_label = flopo_texts[best_idx]\n",
    "                score = round(float(best_score), 4)\n",
    "                method = \"spaCy\"\n",
    "\n",
    "        # Fallback to difflib\n",
    "        if not best_label:\n",
    "            close = get_close_matches(cleaned, flopo_labels.keys(), n=1, cutoff=difflib_cutoff)\n",
    "            if close:\n",
    "                best_label = close[0]\n",
    "                score = None  # difflib does not provide numeric score\n",
    "                method = \"difflib\"\n",
    "\n",
    "        if best_label:\n",
    "            matched.append({\n",
    "                \"trait\": trait,\n",
    "                \"matched_label\": best_label,\n",
    "                \"flopo_uri\": flopo_labels[best_label],\n",
    "                \"method\": method,\n",
    "                \"score\": score\n",
    "            })\n",
    "        else:\n",
    "            matched.append({\n",
    "                \"trait\": trait,\n",
    "                \"matched_label\": None,\n",
    "                \"flopo_uri\": None,\n",
    "                \"method\": None,\n",
    "                \"score\": None\n",
    "            })\n",
    "\n",
    "    return matched\n"
   ]
  },
