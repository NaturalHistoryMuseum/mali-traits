    "def recompute_trait_stats_from_outputs(output_dir=\"output\", num_runs=3):\n",
    "    stats_path = os.path.join(output_dir, \"trait_stats.csv\")\n",
    "\n",
    "    if not os.path.exists(stats_path):\n",
    "        raise FileNotFoundError(f\"Cannot find existing stats file at {stats_path}\")\n",
    "\n",
    "    old_stats_df = pd.read_csv(stats_path)\n",
    "    vote_threshold = math.ceil(num_runs / 2)\n",
    "    new_stats = []\n",
    "\n",
    "    for _, row in old_stats_df.iterrows():\n",
    "        taxon = row[\"taxon\"]\n",
    "        mean_run_len = row[\"mean_run_len\"]\n",
    "        file_path = os.path.join(output_dir, f\"{taxon}.csv\")\n",
    "\n",
    "        if not os.path.exists(file_path):\n",
    "            print(f\"Skipping {taxon} â€” file not found\")\n",
    "            continue\n",
    "\n",
    "        df = pd.read_csv(file_path)\n",
    "        dedup_df = df.drop_duplicates(subset=[\"trait\", \"value\", \"plant_part\", \"phenotypic_trait\"])\n",
    "        total_len = len(dedup_df)\n",
    "\n",
    "        if \"consensus_count\" in df.columns:\n",
    "            full_match = len(dedup_df.query(f\"consensus_count == {num_runs}\"))\n",
    "            consensus = len(dedup_df.query(f\"consensus_count >= {vote_threshold}\"))\n",
    "        elif \"notes\" in df.columns:\n",
    "            full_match = dedup_df[\"notes\"].str.contains(f\"appeared {num_runs} times\").sum()\n",
    "            consensus = dedup_df[\"notes\"].str.contains(f\"appeared {vote_threshold} times\").sum()\n",
    "        else:\n",
    "            full_match = consensus = 0\n",
    "\n",
    "        full_match_pct = round((full_match / mean_run_len) * 100, 2) if mean_run_len else 0\n",
    "        consensus_pct = round((consensus / total_len) * 100, 2) if mean_run_len else 0\n",
    "        conflict_pct = round(100 - consensus_pct, 2)\n",
    "\n",
    "        new_stats.append({\n",
    "            \"taxon\": taxon,\n",
    "            \"mean_run_len\": mean_run_len,\n",
    "            \"total_out\": total_len,\n",
    "            \"full_match_%\": full_match_pct,\n",
    "            \"consensus_%\": consensus_pct,\n",
    "            \"conflict_%\": conflict_pct\n",
    "        })\n",
    "\n",
    "    new_stats_df = pd.DataFrame(new_stats)\n",
    "    new_stats_df.to_csv(stats_path, index=False)\n",
    "    print(f\"âœ… Stats updated and saved to: {stats_path}\")\n",
    "    return new_stats_df\n",
    "\n",
    "stats_df = recompute_trait_stats_from_outputs(output_dir=\"output\", num_runs=3)"
   ]
