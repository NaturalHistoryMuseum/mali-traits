   "source": [
    "def vectorize_labels(flopo_labels, nlp):\n",
    "    vectors = []\n",
    "    texts = []\n",
    "    for label in flopo_labels:\n",
    "        doc = nlp(label)\n",
    "        if doc.has_vector:\n",
    "            vectors.append(doc.vector)\n",
    "            texts.append(label)\n",
    "    return np.array(vectors), texts\n"
   ]
  },
