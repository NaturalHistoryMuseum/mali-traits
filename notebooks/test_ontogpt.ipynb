    "from difflib import get_close_matches\n",
    "\n",
    "def normalize(text):\n",
    "    return text.lower().replace(\" is \", \" \").replace(\" are \", \" \").replace(\":\", \"\").strip()\n",
    "\n",
    "def match_extracted_traits_to_uris(traits, metadata_map):\n",
    "    norm_map = {normalize(label): uri for label, uri in metadata_map.items()}\n",
    "\n",
    "    matches = []\n",
    "    for trait in traits:\n",
