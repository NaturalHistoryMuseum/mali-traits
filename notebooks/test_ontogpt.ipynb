    "def compare_specific_taxa_csvs(dir1, dir2, taxa, threshold=0.8, model_name=\"all-MiniLM-L6-v2\"):\n",
    "    model = SentenceTransformer(model_name)\n",
    "    results = []\n",
    "\n",
    "    for taxon in taxa:\n",
    "        file1 = os.path.join(dir1, f\"{taxon}.csv\")\n",
    "        file2 = os.path.join(dir2, f\"{taxon}.csv\")\n",
    "\n",
    "        if not os.path.exists(file1) or not os.path.exists(file2):\n",
    "            print(f\"âŒ Missing file for taxon: {taxon}\")\n",
    "            continue\n",
    "\n",
    "        df1 = pd.read_csv(file1).dropna(subset=[\"trait\", \"value\", \"plant_part\", \"phenotypic_trait\"])\n",
    "        df2 = pd.read_csv(file2).dropna(subset=[\"trait\", \"value\", \"plant_part\", \"phenotypic_trait\"])\n",
    "\n",
    "        combined1 = df1.apply(lambda row: f\"{row['trait']} | {row['value']} | {row['plant_part']} | {row['phenotypic_trait']}\", axis=1).tolist()\n",
    "        combined2 = df2.apply(lambda row: f\"{row['trait']} | {row['value']} | {row['plant_part']} | {row['phenotypic_trait']}\", axis=1).tolist()\n",
    "\n",
    "        emb1 = model.encode(combined1, convert_to_tensor=True)\n",
    "        emb2 = model.encode(combined2, convert_to_tensor=True)\n",
    "\n",
    "        sim_matrix = util.pytorch_cos_sim(emb1, emb2)\n",
    "\n",
    "        # Many-to-many matches\n",
    "        many_to_many_matches = [\n",
    "            (i, j) for i in range(sim_matrix.shape[0])\n",
    "                   for j in range(sim_matrix.shape[1])\n",
    "                   if sim_matrix[i, j] >= threshold\n",
    "        ]\n",
    "\n",
    "        # One-to-one best matches\n",
    "        matched_i = set()\n",
    "        matched_j = set()\n",
    "        for i in range(sim_matrix.shape[0]):\n",
    "            j = sim_matrix[i].argmax().item()\n",
    "            score = sim_matrix[i, j].item()\n",
    "            if score >= threshold and i not in matched_i and j not in matched_j:\n",
    "                matched_i.add(i)\n",
    "                matched_j.add(j)\n",
    "\n",
    "        row = {\n",
    "            \"taxon\": taxon,\n",
    "            \"rows_in_dir1\": len(df1),\n",
    "            \"rows_in_dir2\": len(df2),\n",
    "            \"many_to_many_matches\": len(many_to_many_matches),\n",
    "            \"best_one_to_one_matches\": len(matched_i),\n",
    "            \"many_to_many_%_vs_dir1\": round(len(many_to_many_matches) / len(df1) * 100, 2) if len(df1) else 0,\n",
    "            \"many_to_many_%_vs_dir2\": round(len(many_to_many_matches) / len(df2) * 100, 2) if len(df2) else 0,\n",
    "            \"best_one_to_one_%_vs_dir1\": round(len(matched_i) / len(df1) * 100, 2) if len(df1) else 0,\n",
    "            \"best_one_to_one_%_vs_dir2\": round(len(matched_j) / len(df2) * 100, 2) if len(df2) else 0,\n",
    "        }\n",
    "\n",
    "        results.append(row)\n",
    "\n",
    "    return pd.DataFrame(results)"
   ]
  },
