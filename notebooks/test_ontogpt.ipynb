    "def load_flopo_labels_rag(owl_path):\n",
    "    g = Graph()\n",
    "    g.parse(owl_path)\n",
    "\n",
    "    IAO = Namespace(\"http://purl.obolibrary.org/obo/IAO_\")\n",
    "    DEFINITION = IAO[\"0000115\"]\n",
    "\n",
    "    data = []\n",
    "    for s in g.subjects(RDFS.label, None):\n",
    "        label = g.value(s, RDFS.label)\n",
    "        desc = g.value(s, DEFINITION)\n",
    "        if label:\n",
    "            data.append({\n",
    "                \"uri\": str(s),\n",
    "                \"label\": str(label),\n",
    "                \"desc\": str(desc) if desc else \"\"\n",
    "            })\n",
    "\n",
    "    flopo_df = pd.DataFrame(data).drop_duplicates().reset_index(drop=True)\n",
    "    return flopo_df"
   ]
