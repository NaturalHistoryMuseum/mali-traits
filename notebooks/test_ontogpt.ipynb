    "def compare_specific_taxa_csvs(dir1, dir2, taxon_list, threshold=0.8):\n",
    "    model = SentenceTransformer(\"all-MiniLM-L6-v2\")\n",
    "    results = []\n",
    "\n",
    "    def row_to_string(row):\n",
    "        return f\"{row['trait']} | {row['value']} | {row['plant_part']} | {row['phenotypic_trait']}\"\n",
    "\n",
    "    for taxon in taxon_list:\n",
    "        filename = f\"{taxon}.csv\"\n",
    "        path1 = os.path.join(dir1, filename)\n",
    "        path2 = os.path.join(dir2, filename)\n",
    "\n",
    "        if not (os.path.exists(path1) and os.path.exists(path2)):\n",
    "            print(f\"Missing file for {taxon} in one of the directories.\")\n",
    "            continue\n",
    "\n",
    "        try:\n",
    "            df1 = pd.read_csv(path1).dropna(subset=[\"trait\", \"value\", \"plant_part\", \"phenotypic_trait\"])\n",
    "            df2 = pd.read_csv(path2).dropna(subset=[\"trait\", \"value\", \"plant_part\", \"phenotypic_trait\"])\n",
    "        except Exception as e:\n",
    "            print(f\"Failed to read data for {taxon}: {e}\")\n",
    "            continue\n",
    "\n",
    "        strings1 = df1.apply(row_to_string, axis=1).tolist()\n",
    "        strings2 = df2.apply(row_to_string, axis=1).tolist()\n",
    "\n",
    "        emb1 = model.encode(strings1, convert_to_tensor=True)\n",
    "        emb2 = model.encode(strings2, convert_to_tensor=True)\n",
    "\n",
    "        sim_matrix = util.cos_sim(emb1, emb2)\n",
    "\n",
    "        matched_pairs = set()\n",
    "        for i, row in enumerate(sim_matrix):\n",
    "            for j, sim in enumerate(row):\n",
    "                if sim >= threshold:\n",
    "                    matched_pairs.add((i, j))\n",
    "\n",
    "        results.append({\n",
    "            \"taxon\": taxon,\n",
    "            \"matched_rows\": len(matched_pairs),\n",
    "            \"rows_in_dir1\": len(df1),\n",
    "            \"rows_in_dir2\": len(df2),\n",
    "            \"match_%_vs_dir1\": round(len(matched_pairs) / len(df1) * 100, 2),\n",
    "            \"match_%_vs_dir2\": round(len(matched_pairs) / len(df2) * 100, 2)\n",
    "        })\n",
    "\n",
    "    return pd.DataFrame(results)"
   ]
