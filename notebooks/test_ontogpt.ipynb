    "from rapidfuzz import fuzz\n",
    "from collections import Counter\n",
    "from sentence_transformers import SentenceTransformer, util\n",
    "from functools import lru_cache\n",
    "\n",
    "\n",
    "# Expanded canonical plant parts list\n",
    "canonical_parts = [\n",
    "    \"leaf\", \"leaf blade\", \"leaf sheath\", \"root\", \"stem\", \"node\", \"internode\",\n",
    "    \"flower\", \"fruit\", \"culm\", \"panicle\", \"spikelet\", \"awn\", \"ligule\",\n",
    "    \"palea\", \"lemma\", \"glume\", \"anther\", \"stigma\", \"style\", \"ovary\",\n",
    "    \"bract\", \"rachis\", \"blade\", \"petiole\", \"inflorescence\", \"tip\", \"base\", \"axil\"\n",
    "]\n",
    "\n",
    "# Aliases and plurals, for secure mapping if LLM does not map plant part to singular\n",
    "alias_map = {\n",
    "    \"leaves\": \"leaf\",\n",
    "    \"leaf sheaths\": \"leaf sheath\",\n",
    "    \"sheath\": \"leaf sheath\",\n",
    "    \"culms\": \"culm\",\n",
    "    \"nodes\": \"node\",\n",
    "    \"internodes\": \"internode\",\n",
    "    \"flowers\": \"flower\",\n",
    "    \"fruits\": \"fruit\",\n",
    "    \"roots\": \"root\",\n",
    "    \"stems\": \"stem\",\n",
    "    \"panicles\": \"panicle\",\n",
    "    \"spikelets\": \"spikelet\",\n",
    "    \"awns\": \"awn\",\n",
    "    \"ligules\": \"ligule\",\n",
    "    \"paleas\": \"palea\",\n",
    "    \"lemmas\": \"lemma\",\n",
    "    \"glumes\": \"glume\",\n",
    "    \"anthers\": \"anther\",\n",
    "    \"stigmas\": \"stigma\",\n",
    "    \"styles\": \"style\",\n",
    "    \"ovaries\": \"ovary\",\n",
    "    \"bracts\": \"bract\",\n",
    "    \"rachises\": \"rachis\",\n",
    "    \"blades\": \"blade\",\n",
    "    \"petioles\": \"petiole\",\n",
    "    \"inflorescences\": \"inflorescence\",\n",
    "    \"bases\": \"base\",\n",
    "    \"tips\": \"tip\",\n",
    "    \"axils\": \"axil\"\n",
    "}\n",
    "\n",
    "def normalise_plant_part_fuzzy(part, threshold=90):\n",
    "    \"\"\"\n",
    "    Fuzzy match a raw plant part string to a list of canonical terms.\n",
    "    Handles NaN/missing values safely.\n",
    "    \"\"\"\n",
    "    if not isinstance(part, str):\n",
    "        part = str(part) if part is not None else \"\"\n",
    "    return _normalise_part_cached(part.strip().lower())\n",
    "\n",
    "@lru_cache(maxsize=None)\n",
    "def _normalise_part_cached(part):\n",
    "    if part in alias_map:\n",
    "        return alias_map[part]\n",
    "    if part in canonical_parts:\n",
    "        return part\n",
    "    scores = [(cand, fuzz.ratio(part, cand)) for cand in canonical_parts]\n",
    "    best_match, score = max(scores, key=lambda x: x[1])\n",
    "    return best_match if score >= 90 else part\n"
   ]
