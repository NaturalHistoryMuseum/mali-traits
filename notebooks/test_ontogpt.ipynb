    "from difflib import get_close_matches\n",
    "\n",
    "def clean_trait(trait):\n",
    "    return trait.replace(\" is\", \"\").replace(\" are\", \"\").replace(\":\", \"\").strip().lower()\n",
    "\n",
    "def match_traits_to_flopo(traits, flopo_labels):\n",
    "    matched = []\n",
    "    for trait in traits:\n",
    "        cleaned = clean_trait(trait)\n",
    "        match = get_close_matches(cleaned, flopo_labels.keys(), n=1, cutoff=0.6)\n",
    "        if match:\n",
    "            matched.append({\n",
    "                \"trait\": trait,\n",
    "                \"matched_label\": match[0],\n",
    "                \"flopo_uri\": flopo_labels[match[0]]\n",
    "            })\n",
    "        else:\n",
    "            matched.append({\n",
    "                \"trait\": trait,\n",
    "                \"matched_label\": None,\n",
    "                \"flopo_uri\": None\n",
    "            })\n",
    "    return matched\n"
   ]
