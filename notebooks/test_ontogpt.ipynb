    "def evaluate_llm_mappings_vs_faiss(input_dir):\n",
    "    summary = []\n",
    "\n",
    "    for file in os.listdir(input_dir):\n",
    "        if not file.endswith(\".csv\") or file == \"trait_stats.csv\":\n",
    "            continue\n",
    "\n",
    "        filepath = os.path.join(input_dir, file)\n",
    "        try:\n",
    "            df = pd.read_csv(filepath)\n",
    "        except Exception as e:\n",
    "            print(f\"Could not read {file}: {e}\")\n",
    "            continue\n",
    "\n",
    "        required_cols = {\"llm_mapped_term\", \"flopo_id\", \"pato_id\"}\n",
    "        if not required_cols.issubset(set(df.columns)):\n",
    "            print(f\"Missing required columns in {file}\")\n",
    "            continue\n",
    "\n",
    "        taxon = os.path.splitext(file)[0]\n",
    "        total = len(df)\n",
    "\n",
    "        exact_flopo_match = (df[\"llm_mapped_term\"] == df[\"flopo_id\"]).sum()\n",
    "        exact_pato_match = (df[\"llm_mapped_term\"] == df[\"pato_id\"]).sum()\n",
    "        total_exact = exact_flopo_match + exact_pato_match\n",
    "        unmatched = total - total_exact\n",
    "\n",
    "        summary.append({\n",
    "            \"taxon\": taxon,\n",
    "            \"total_rows\": total,\n",
    "            \"matched_flopo\": exact_flopo_match,\n",
    "            \"matched_pato\": exact_pato_match,\n",
    "            \"matched_total\": total_exact,\n",
    "            \"unmatched\": unmatched,\n",
    "            \"matched_%\": round(total_exact / total * 100, 2),\n",
    "            \"flopo_%\": round(exact_flopo_match / total * 100, 2),\n",
    "            \"pato_%\": round(exact_pato_match / total * 100, 2)\n",
    "        })\n",
    "\n",
    "    return pd.DataFrame(summary)"
   ]
  },
