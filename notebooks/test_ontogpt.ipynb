    "import spacy\n",
    "from sklearn.metrics.pairwise import cosine_similarity\n",
    "import numpy as np\n",
    "\n",
    "# Load spaCy model\n",
    "nlp = spacy.load(\"en_core_web_md\")\n",
    "\n",
    "def precompute_flopo_vectors(flopo_labels):\n",
    "    flopo_texts = list(flopo_labels.keys())\n",
    "    flopo_vectors = np.array([nlp(text).vector for text in flopo_texts])\n",
    "    return flopo_texts, flopo_vectors\n",
    "\n",
    "def match_traits_to_flopo_spaCy(traits, flopo_labels, flopo_texts, flopo_vectors, spaCy_threshold=0.75):\n",
    "    matched = []\n",
    "    for trait in traits:\n",
    "        cleaned = clean_trait(trait)\n",
    "        trait_vec = nlp(cleaned).vector.reshape(1, -1)\n",
    "\n",
    "        sim_scores = cosine_similarity(trait_vec, flopo_vectors)[0]\n",
    "        best_idx = np.argmax(sim_scores)\n",
    "        best_score = sim_scores[best_idx]\n",
    "        best_label = flopo_texts[best_idx]\n",
    "\n",
    "        if best_score >= spaCy_threshold:\n",
    "            matched.append({\n",
    "                \"trait\": trait,\n",
    "                \"matched_label\": best_label,\n",
    "                \"flopo_uri\": flopo_labels[best_label],\n",
    "                \"method\": \"spaCy\",\n",
    "                \"score\": round(float(best_score), 3)\n",
    "            })\n",
    "        else:\n",
    "            fallback = get_close_matches(cleaned, flopo_labels.keys(), n=1, cutoff=0.6)\n",
    "            if fallback:\n",
    "                matched.append({\n",
    "                    \"trait\": trait,\n",
    "                    \"matched_label\": fallback[0],\n",
    "                    \"flopo_uri\": flopo_labels[fallback[0]],\n",
    "                    \"method\": \"difflib\",\n",
    "                    \"score\": None\n",
    "                })\n",
    "            else:\n",
    "                matched.append({\n",
    "                    \"trait\": trait,\n",
    "                    \"matched_label\": None,\n",
    "                    \"flopo_uri\": None,\n",
    "                    \"method\": \"none\",\n",
    "                    \"score\": None\n",
    "                })\n",
    "    return matched\n"
   ]
  },
