    "import pandas as pd\n",
    "from sentence_transformers import SentenceTransformer\n",
    "import faiss\n",
    "import numpy as np\n",
    "\n",
    "# Sample ontology entries\n",
    "ontology_data = [\n",
    "    {\n",
    "        \"flopo_id\": \"FLOPO:0022609\",\n",
    "        \"label\": \"tepal coiled\",\n",
    "        \"desc\": \"tepal that bears coiled trait\",\n",
    "        \"po_id\": \"PO:0009033\",\n",
    "        \"po_label\": \"tepal\",\n",
    "        \"pato_id\": \"PATO:0000404\",\n",
    "        \"pato_label\": \"coiled\"\n",
    "    },\n",
    "    {\n",
    "        \"flopo_id\": \"FLOPO:0000013\",\n",
    "        \"label\": \"plant is perennial\",\n",
    "        \"desc\": \"A plant with a perennial life cycle\",\n",
    "        \"po_id\": \"PO:0000003\",\n",
    "        \"po_label\": \"plant\",\n",
    "        \"pato_id\": \"PATO:0001309\",\n",
    "        \"pato_label\": \"perennial\"\n",
    "    },\n",
    "    {\n",
    "        \"flopo_id\": \"FLOPO:0900032\",\n",
    "        \"label\": \"plant is creeping\",\n",
    "        \"desc\": \"A plant with creeping growth form\",\n",
    "        \"po_id\": \"PO:0000003\",\n",
    "        \"po_label\": \"plant\",\n",
    "        \"pato_id\": \"PATO:0001443\",\n",
    "        \"pato_label\": \"creeping\"\n",
    "    }\n",
    "]\n",
    "\n",
    "# Convert to DataFrame\n",
    "ontology_df = pd.DataFrame(ontology_data)\n",
    "\n",
    "# Combine textual content into a hybrid search string\n",
    "ontology_df[\"search_text\"] = (\n",
    "    ontology_df[\"label\"] + \" ; \" +\n",
    "    ontology_df[\"desc\"] + \" ; \" +\n",
    "    ontology_df[\"po_label\"] + \" ; \" +\n",
    "    ontology_df[\"pato_label\"]\n",
    ")\n",
    "\n",
    "# Load embedding model\n",
    "model = SentenceTransformer(\"all-MiniLM-L6-v2\")\n",
    "\n",
    "# Encode the search_text\n",
    "embeddings = model.encode(ontology_df[\"search_text\"].tolist(), convert_to_numpy=True)\n",
    "\n",
    "# Build FAISS index\n",
    "dimension = embeddings.shape[1]\n",
    "faiss_index = faiss.IndexFlatL2(dimension)\n",
    "faiss_index.add(embeddings)\n",
    "\n",
    "# Save for user interaction\n",
    "ontology_df[\"embedding_index\"] = list(range(len(ontology_df)))\n",
    "\n",
    "import ace_tools as tools; tools.display_dataframe_to_user(name=\"Hybrid Ontology FAISS Index\", dataframe=ontology_df)\n"
   ]
  }
