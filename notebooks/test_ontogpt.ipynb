    "def ground_to_flopo(descriptor, ontology, plant_part=None):\n",
    "    import re\n",
    "\n",
    "    if not descriptor:\n",
    "        return (\"NO DESCRIPTOR\", \"NO LABEL\")\n",
    "\n",
    "    descriptor = descriptor.lower()\n",
    "    traits = [t.strip() for t in re.split(r\",|;|/|\\band\\b|\\bwith\\b|\\band\\b\", descriptor, flags=re.IGNORECASE)]\n",
    "\n",
    "    candidates = []\n",
    "\n",
    "    for trait in traits:\n",
    "        for term in ontology.terms():\n",
    "            if not term.name or not term.id.startswith(\"FLOPO:\"):\n",
    "                continue\n",
    "\n",
    "            term_label = term.name.lower()\n",
    "\n",
    "            # Match quality score\n",
    "            score = 0\n",
    "            if trait in term_label:\n",
    "                score += 1\n",
    "            if plant_part and plant_part.lower() in term_label:\n",
    "                score += 2  # boost if both match\n",
    "            for syn in term.synonyms:\n",
    "                if syn.description and trait in syn.description.lower():\n",
    "                    score += 0.5\n",
    "\n",
    "            if score > 0:\n",
    "                candidates.append((score, term.id, term.name))\n",
    "\n",
    "    if not candidates:\n",
    "        return (\"NOT FOUND\", \"NO MATCH\")\n",
    "\n",
    "    # Sort by score descending\n",
    "    best_match = sorted(candidates, key=lambda x: -x[0])[0]\n",
    "    return (best_match[1], best_match[2])  # (id, name)\n"
